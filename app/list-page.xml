<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="onNavigatingTo" class="page">

	<Page.actionBar>
		<ActionBar title="ListViewTest" icon="" class="action-bar">
		</ActionBar>
	</Page.actionBar>
	<ListView items="{{ myItems }}" itemTap="itemTap" class="list-group">
		<ListView.itemTemplate>
			<Label class="list-group-item" text="{{ $value }}"/>
		</ListView.itemTemplate>
	</ListView>
</Page>
